<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="HomePageController">
    <bottom>
        <HBox alignment="CENTER" prefHeight="101.0" prefWidth="563.0" spacing="30.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="loginButton" mnemonicParsing="false" onAction="#goToLoginPage" prefHeight="30.0"
                        prefWidth="140.0" text="Login"/>
                <Button fx:id="browseResourcesButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        onAction="#goToInventoryBrowser" prefHeight="30.0" prefWidth="140.0" text="Browse resources"/>
            </children>
        </HBox>
    </bottom>
    <center>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="195.0" fitWidth="331.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@HomePage.png"/>
                    </image>
                </ImageView>
            </children>
        </HBox>
    </center>
</BorderPane>